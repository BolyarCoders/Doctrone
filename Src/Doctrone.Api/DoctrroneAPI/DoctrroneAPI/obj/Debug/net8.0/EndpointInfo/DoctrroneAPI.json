{
  "openapi": "3.0.1",
  "info": {
    "title": "DoctrroneAPI",
    "version": "1.0"
  },
  "paths": {
    "/Doctrone/GetUsers": {
      "get": {
        "tags": [
          "Doctrone"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Doctrone/Login": {
      "post": {
        "tags": [
          "Doctrone"
        ],
        "parameters": [
          {
            "name": "email",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Doctrone/Register": {
      "post": {
        "tags": [
          "Doctrone"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserRegister"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserRegister"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserRegister"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserRegister": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "pass": {
            "type": "string",
            "nullable": true
          },
          "bloodType": {
            "type": "string",
            "nullable": true
          },
          "age": {
            "type": "integer",
            "format": "int64"
          },
          "gender": {
            "type": "string",
            "nullable": true
          },
          "specialDiagnosis": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}